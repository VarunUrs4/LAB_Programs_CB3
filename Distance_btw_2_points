#include<stdio.h>
#include<math.h>

void main()
{
    float x1 ,x2 ,y1 ,y2 ,d;
    printf("\n Enter the first point values : ");
    scanf("%f %f", &x1 ,&y1);
    printf("\n Enter the second point values : ");
    scanf("%f %f", &x2 ,&y2);
    d = sqrt((x2-x1)*(x2-x1) + (y2-y1)*(y2-y1));
    printf("\n The distance between 2 points is %f",d);
    return 0;
}
